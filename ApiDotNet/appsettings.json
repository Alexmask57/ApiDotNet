{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CORSOrigin": [
    "http://localhost:4200"
  ],
  "CorsPolicyName": "ApiDotNetCorsPolicy",
  "ConnectionStrings": {
    "MYSQL_PASSWORD": "{{MYSQL_PASSWORD}}",
    "MYSQL_USER": "{{MYSQL_USER}}",
    "MYSQL_DATABASE": "JWTAuthentication",
  },
  "JwtSecret": "{{JWT_SECRET}}"
}
